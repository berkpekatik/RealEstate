@model List<RealEstate.Models.MemberModel>
@{
    ViewBag.Title = "Üye Listesi";
    ViewBag.Description = "Tüm Üyelerin listesi";
}
<section class="content">
    <div class="row">
        <div class="col-xs-12">
            <div class="box">
                <div class="box-header">
                    <h3 class="box-title">Aşağıdaki tablodan <b>Üyeleri</b> görebilir ve işlem yapabilirsiniz.</h3>
                </div>
                <!-- /.box-header -->
                <div class="box-body">
                    <table id="example2" class="table table-bordered table-hover">
                        <thead>
                            <tr>
                                <th>Kullanıcı Adı</th>
                                <th>Şifre</th>
                                <th>E-mail</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@item.Username</td>
                                    <td>@item.Password</td>
                                    <td>@item.Email</td>
                                    <td>
                                        <a href="@Url.Action("Edit", new { id = item.MemberId })">Düzenle (Çalışmıyor)</a>
                                        /
                                        <a href="@Url.Action("Delete", new { id = item.MemberId })">Sil (Çalışmıyor)</a>

                                    </td>
                                </tr>

                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</section>